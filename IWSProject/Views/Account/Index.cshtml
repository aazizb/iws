@using IWSProject.Content
<script type="text/javascript">
    function OnRowClicked(s, e, url) {

        var destUrl = url + "/" + s.GetRowKey(e.visibleIndex);
        window.location.href = destUrl;
    }
</script>

@{
    ViewBag.Title = IWSLocalResource.RolesAssignement;
}
<div class="accountHeader">
    <h2>@ViewBag.Title</h2>
</div>
@Html.Partial("UsersList")

